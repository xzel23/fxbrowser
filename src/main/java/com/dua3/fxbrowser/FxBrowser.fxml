<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>


<BorderPane prefHeight="528.0" prefWidth="655.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <top>
                  <HBox>
                     <children>
      <ButtonBar prefHeight="40.0" BorderPane.alignment="CENTER" buttonMinWidth="20.0">
        <buttons>
          <Button fx:id="btnPrev" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="prev">
               <graphic>
                  <FontAwesomeIconView glyphName="ARROW_LEFT" size="2em" />
               </graphic>
            </Button>
            <Button fx:id="btnNext" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="next">
               <graphic>
                  <FontAwesomeIconView glyphName="ARROW_RIGHT" size="2em" />
               </graphic>
            </Button>
            <Button fx:id="btnStopReload" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" text="stop/reload">
               <graphic>
                  <FontAwesomeIconView glyphName="REFRESH" size="2em" />
               </graphic>
            </Button>
        </buttons>
      </ButtonBar>
        <TextField fx:id="inputURL" />
        </children>
        </HBox>
   </top>
   <center>
      <WebView fx:id="webview" prefHeight="383.0" prefWidth="622.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
